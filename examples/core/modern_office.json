                  { "UILabel": { "StringLiteral": "Enter your notes below:" } },
                  { "UITextInput": { "StringLiteral": "input_text" } },
                  {
                    "If": [
                      { "Eq": [ { "UIButton": { "StringLiteral": "Clear" } }, { "IntLiteral": 1 } ] },
                      { "Assign": [ "input_text", { "StringLiteral": "" } ] },
                      null
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  ]
}
